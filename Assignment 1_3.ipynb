   "source": [
    "Num=(range(1,10))\n",
    "even_count = 0\n",
    "odd_count = 0\n",
    "for s in Num:\n",
    "    if s %2 == 0:\n",
    "        even_count += 1\n",
    "    else:\n",
    "        odd_count += 1\n",
    "print(\"Number of even numbers\" , even_count)\n",
    "print(\"Number of odd numbers\" , odd_count)"
   ]
